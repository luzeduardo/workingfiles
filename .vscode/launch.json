  "version": "0.2.0",
  "configurations": [
      {
        "name": "Launch workingfiles CMD",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "cwd": "${workspaceRoot}",
        "program": "./mdp/",
        "args": [
          "-file",
          "./mdp/README.md",
          "-t",
          "./mdp/template-fmt.html.tmpl"
        ],
        "env": {},
        "showLog": true
      },
      {
        "name": "CMDGO Test Current File",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}/${relativeFileDirname}",
        "showLog": true        
      }
  ]
}